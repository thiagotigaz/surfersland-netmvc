@model IEnumerable<SurfersLand.Models.Board>

@{
    ViewBag.Title = "Boards";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
<table class="table table-bordered table-hover">
    <thead>
    <tr>
        <th>Boards</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var board in Model)
    {
        <tr>
            <td>@board.Name</td>
        </tr>
    }
    </tbody>
</table>